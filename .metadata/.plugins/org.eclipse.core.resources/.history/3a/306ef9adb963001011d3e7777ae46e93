package learn.lamdaexpression;

import learn.lamdaexpression.interfc.IVoid;

public class GetNameUsingVoidInterface {

	public static void main(String[] args) {

		IVoid test = (str) -> {
			System.out.println("hello how are you " + str);
		};

		test.getName("Lingadharan");
		
		// we can also use like this 
		
		IVoid test2 = (str)-> System.out.println("hey there how are you "+ str);
		
		process(test2);
	}

	public static void process(IVoid greetings) {
		greetings.getName("Lucky");
	}
}
