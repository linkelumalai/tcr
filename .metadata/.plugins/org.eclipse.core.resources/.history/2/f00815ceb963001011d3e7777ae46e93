package learn.lamdaexpression;

import learn.lamdaexpression.interfc.IAddFI;

public class SumUsingIAddFI {

	public static void main(String[] args) {

		System.out.println();
	}

	public static int add(int a, int b) {

		IAddFI add = () -> {
			int sum = a + b;
			return sum;
		};

		return add.g;
	}
}
