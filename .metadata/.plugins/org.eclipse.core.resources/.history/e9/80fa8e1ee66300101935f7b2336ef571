package learn.lamdaexpression;

import learn.lamdaexpression.interfc.IArithmetic;

public class SumUsingIAddFI {

	public static void main(String[] args) {

		IArithmetic arithmaticOperation = (a, b) -> a + b; // it is also valid expression
		// because it is single statement

		IArithmetic oldArithmetic = (a, b) -> {
			int sum = a + b;
			return sum;
		};
		
		System.out.println(oldArithmetic.sum(20, 30));
		
		System.out.println(arithmaticOperation.sum(20, 90));
	}

}
