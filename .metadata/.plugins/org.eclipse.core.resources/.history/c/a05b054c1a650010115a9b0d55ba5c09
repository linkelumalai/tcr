package learn.lamdaexpression;

public class ThisDemoLamdaExpression {

	public static void main(String[] args) {

	}

	public Printer getLamda(String input) {
		Printer print = (intput) -> System.out.println(input + " : " + this.getClass());
		return print;
	}

	public Printer getAnonymous(String input) {
		Printer print = new Printer() {

			@Override
			public void printer(String input) {
				System.out.println(input + " : " + this.getClass());
			}
		};

		return print;
	}
}
